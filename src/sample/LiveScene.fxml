<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LiveSceneController">
   <children>
      <TabPane fx:id="tabPane" layoutX="35.0" layoutY="189.0" prefHeight="317.0" prefWidth="378.0" side="LEFT" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="introTag" text="Intro">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="281.0" prefWidth="348.0">
                     <children>
                        <TextArea layoutY="-5.0" prefHeight="318.0" prefWidth="355.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <DatePicker fx:id="datePicker" layoutX="37.0" layoutY="536.0" promptText="pick a date for your live session" />
      <ChoiceBox fx:id="timePicker" layoutX="228.0" layoutY="536.0" prefHeight="23.0" prefWidth="186.0" />
      <Button fx:id="subscribeButton" layoutX="34.0" layoutY="141.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="163.0" text="Subscribe ">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" layoutX="219.0" layoutY="141.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="196.0" text="Delete">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ScrollPane layoutX="421.0" layoutY="189.0" prefHeight="200.0" prefWidth="378.0">
         <content>
            <TextArea fx:id="personalPlanTextArea" prefHeight="200.0" prefWidth="362.0" />
         </content>
      </ScrollPane>
      <Label layoutX="552.0" layoutY="164.0" text="Personal Plan">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
